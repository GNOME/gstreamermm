#include <gst/gstcaps.h>

namespace Gst
{

Glib::RefPtr<Caps>
Caps::create_empty()
{
  return Glib::wrap(gst_caps_new_empty(), false);
}

Glib::RefPtr<Caps>
Caps::create_any()
{
  return Glib::wrap(gst_caps_new_any(), false);
}

Glib::RefPtr<Caps>
Caps::create_full(Structure& structure)
{
  return Glib::wrap(gst_caps_new_full(structure.gobj()), false);
}


} //namespace Gst
